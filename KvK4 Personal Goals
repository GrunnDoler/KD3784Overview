        ];

        // Format number with commas
        function formatNumber(num) {
            return num.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }

        // Populate table
        function populateTable(data = governorData) {
            const tableBody = document.getElementById('tableBody');
            tableBody.innerHTML = '';
            
            data.forEach((gov, index) => {
                const row = document.createElement('tr');
                row.innerHTML = `
                    <td class="rank">${index + 1}</td>
                    <td>${gov.id}</td>
                    <td class="governor-name">${gov.name}</td>
                    <td class="number">${formatNumber(gov.power)}</td>
                    <td class="number">${formatNumber(gov.kvkKpGoal)}</td>
                    <td class="number">${formatNumber(gov.kvkDeathGoal)}</td>
                    <td class="number">${formatNumber(gov.kvkScoreGoal)}</td>
                `;
                tableBody.appendChild(row);
            });
        }

        // Search functionality
        document.getElementById('searchInput').addEventListener('keyup', function(e) {
            const searchTerm = e.target.value.toLowerCase();
            const filtered = governorData.filter(gov => 
                gov.name.toLowerCase().includes(searchTerm) || 
                gov.id.toString().includes(searchTerm)
            );
            populateTable(filtered);
        });

        // Calculate statistics
        function calculateStats() {
            const avgPower = Math.floor(governorData.reduce((sum, gov) => sum + gov.power, 0) / governorData.length);
            document.getElementById('avgPower').textContent = formatNumber(avgPower);
            document.getElementById('totalGovs').textContent = governorData.length;
            document.getElementById('topPower').textContent = formatNumber(governorData[0].power);
        }

        // Initialize
        populateTable();
        calculateStats();
    </script>
